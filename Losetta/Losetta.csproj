<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <TargetFramework>net6.0</TargetFramework>
    <Company>WSOFT</Company>
    <Authors>WSOFT</Authors>
    <Product>Losetta</Product>
    <RepositoryUrl>https://github.com/WSOFT-Project/Losetta</RepositoryUrl>
    <PackageProjectUrl>https://docs.wsoft.ws/producs/alice</PackageProjectUrl>
    <Description>The next generation implementation of alice</Description>
    <SignAssembly>False</SignAssembly>
    <Version>0.9.13</Version>
    <ApplicationIcon>alice_logo.ico</ApplicationIcon>
    <Copyright>Copyright (c) 2022 WSOFT All Rights Reserved.</Copyright>
    <PackageIcon>alice_logo.png</PackageIcon>
    <Title>Losetta</Title>
    <PackageTags>Scripting</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="alice_logo.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\WSOFT_Documents\Alice\alice_logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="7.0.0" />
  </ItemGroup>

</Project>
